<?xml version='1.0' encoding='utf-8'?>
<widget id="com.scanner.scanTracker" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="ios-package" value="CDVBarcodeScanner" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="CameraUsesGeolocation" value="false" />
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Capture">
        <param name="ios-package" value="CDVCapture" />
    </feature>
    <feature name="Media">
        <param name="ios-package" value="CDVSound" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="Contacts">
        <param name="ios-package" value="CDVContacts" />
    </feature>
    <feature name="Globalization">
        <param name="ios-package" value="CDVGlobalization" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="Battery">
        <param name="ios-package" value="CDVBattery" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <icon height="60" src="platforms/ios/ScanTracker/Resources/icons/icon-60.png" width="60" />
    <icon height="120" src="platforms/ios/ScanTracker/Resources/icons/icon-60@2x.png" width="120" />
    <icon height="76" src="platforms/ios/ScanTracker/Resources/icons/icon-76.png" width="76" />
    <icon height="152" src="platforms/ios/ScanTracker/Resources/icons/icon-76@2x.png" width="152" />
    <icon height="40" src="platforms/ios/ScanTracker/Resources/icons/icon-40.png" width="40" />
    <icon height="80" src="platforms/ios/ScanTracker/Resources/icons/icon-40@2x.png" width="80" />
    <icon height="57" src="platforms/ios/ScanTracker/Resources/icons/icon.png" width="57" />
    <icon height="114" src="platforms/ios/ScanTracker/Resources/icons/icon@2x.png" width="114" />
    <icon height="72" src="platforms/ios/ScanTracker/Resources/icons/icon-72.png" width="72" />
    <icon height="144" src="platforms/ios/ScanTracker/Resources/icons/icon-72@2x.png" width="144" />
    <icon height="29" src="platforms/ios/ScanTracker/Resources/icons/icon-small.png" width="29" />
    <icon height="58" src="platforms/ios/ScanTracker/Resources/icons/icon-small@2x.png" width="58" />
    <icon height="50" src="platforms/ios/ScanTracker/Resources/icons/icon-50.png" width="50" />
    <icon height="100" src="platforms/ios/ScanTracker/Resources/icons/icon-50@2x.png" width="100" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <name>ScanTracker</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <icon height="200" src="res/icon.png" width="170" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
